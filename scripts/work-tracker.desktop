[Desktop Entry]
Type=Application
Name=Work Tracker
Comment=Start the Work Tracker application
Exec=/usr/bin/env bash -lc 'nohup ${WT_ROOT}/scripts/work-tracker.sh >/tmp/work-tracker.log 2>&1 & disown'
Path=${WT_ROOT}
Terminal=false
Categories=Office;Utility;
Icon=${WT_ROOT}/pictures/work-tracker-icon.png